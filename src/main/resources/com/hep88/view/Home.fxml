<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane maxHeight="470.0" maxWidth="800.0" minHeight="470.0" minWidth="800.0" prefHeight="470.0" prefWidth="800.0"
            style="-fx-background-color: #FAF2E9;" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hep88.view.HomeController">
    <children>
        <ImageView fitHeight="531.0" fitWidth="926.0" layoutX="-53.0">
            <image>
                <Image url="file:src/main/resources/com/hep88/view/HomeBackground.png"/>
            </image>
        </ImageView>
        <Button layoutX="355.0" layoutY="251.0" mnemonicParsing="false" onAction="#handleShowLobby"
                style="-fx-background-color: black;" text="Start Game" textFill="WHITE">
            <font>
                <Font name="Chalkboard Bold" size="13.0"/>
            </font>
        </Button>
        <Button layoutX="354.0" layoutY="290.0" mnemonicParsing="false" onAction="#handleShowInstruction"
                style="-fx-background-color: black;" text="How to Play" textFill="WHITE">
            <font>
                <Font name="Chalkboard Bold" size="13.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
